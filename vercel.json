{
  "buildCommand": "cd frontend && npm install && npm run build",
  "outputDirectory": "frontend/dist",
  "framework": null,
  "rewrites": [
    { "source": "/api/prices/:coinId", "destination": "/api/prices/:coinId" },
    { "source": "/api/prices", "destination": "/api/prices" },
    { "source": "/api/chart/:coinId", "destination": "/api/chart/:coinId" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
